{
  "name": "backstage-ui",
  "version": "1.0.0",
  "description": "A backstage ui project of React",
  "private": true,
  "author": "Tan90Qian <1436811949@qq.com>",
  "scripts": {
    "start": "cross-env MOCK=true webpack-dev-server --inline --progress --config config/webpack.dev.conf.js",
    "start:no-mock": "webpack-dev-server --inline --progress --config config/webpack.dev.conf.js",
    "build": "node config/build.js",
    "lint": "eslint --ext .ts,.tsx src && npm run lint-style",
    "lint:fix": "eslint --fix --ext .ts,.tsx src && npm run lint-style:fix",
    "lint-style": "stylelint \"src/**/*.{less,css}\"",
    "lint-style:fix": "stylelint \"src/**/*.{less,css}\" --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,less,css}\"",
    "test": "jest"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.8",
    "@types/enquire.js": "^2.1.2",
    "@types/lodash": "^4.14.133",
    "@types/mockjs": "^1.0.2",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^4.3.3",
    "antd": "^3.19.2",
    "axios": "^0.18.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.4.0",
    "mockjs": "^1.0.1-beta3",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "autoprefixer": "^9.5.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-import-plugin": "^1.5.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
