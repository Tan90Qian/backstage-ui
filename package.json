{
  "name": "backstage-ui",
  "version": "1.0.0",
  "description": "A react's backstage project",
  "private": true,
  "author": "Tan90Qian <1436811949@qq.com>",
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "webpack-dev-server --inline --progress --config config/webpack.dev.conf.js",
    "build": "node config/build.js",
    "lint": "eslint --ext .js,.jsx src && npm run lint-style",
    "lint:fix": "eslint --fix --ext .js,.jsx src && npm run lint-style:fix",
    "lint-style": "stylelint \"src/**/*.{scss,css}\"",
    "lint-style:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,scss,css}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "antd": "^3.18.2",
    "react": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.11.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "**/*.{scss,css}": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ]
    },
    "ignore": [
      "dist/**/*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
