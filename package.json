    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{less,css}": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ]
    },
    "ignore": [
      "dist/**/*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
